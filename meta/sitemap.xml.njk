---
permalink: /sitemap.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{%- for item in collections.pages %}
 {%- if not item.data.sitemapIgnore %}
    <url>
        <loc>https://nixinova.com{{ item.data.permalink | replace('.html', '') }}</loc>
        <changefreq>always</changefreq>
        {% if item.data.date %}<lastmod>{{ item.data.date }}</lastmod>{%- endif %}
    </url>
 {%- endif %}
{%- endfor %}
</urlset>
